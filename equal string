import java.util.Scanner;
public class email
{
    public static void main(String args[])
    {
        String s1,s2;
        Scanner sc=new Scanner(System.in);
        System.out.print("enter email id:");
        s1=sc.nextLine();
        System.out.print("Re-enter the email id:");
        s2=sc.nextLine();
        Boolean b=s1.equals(s2);
        System.out.print(b);
    }
}
